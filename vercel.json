{
  "builds": [
    { "src": "**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/(.*)\\.(css|js|png|jpg|jpeg|ico|svg|gif|webp|woff|woff2|ttf)$",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}